while ultrasound_front > 20:
    print(ultrasound_front)
    update_sensors()
    motor_on(60, 60)
    time.sleep(0.05)
    ultrasound_front = front_sensor
motor_off()

# Obstacle detected
if ultrasound_front < 10:
    motors_spin(.48, 70, 70,kick_time=0.1,kick_left_strength=90,kick_right_strength=100)
    motor_on(40,40)
    time.sleep(1.5)
    motor_off()
#turn left 90 degrees
motors_spin(.65, -70, -70, kick_time=0.1, kick_left_strength=90,kick_right_strength=100)
